#include<cstdio>
#include<cstdlib>
#include<algorithm>
#include<cstring>
#include<bitset>
#include<cmath>
using namespace std;
const int maxn= 3001;
int bl,x;
int main()
{
	freopen("1.in","r",stdin);
	freopen("1.out","w",stdout);
	bitset <1000>  a(0);
	a[0]= 1;
	bl= a.test(0);
	bl= a.none();
	bl= a.any();
	a[0]= a[1]= a[1000]= 1;
	x= a.count();
	x= a.size();
	a.flip();
	a.flip(0);
	a[0].flip();
	a[0]= 1;
	a.set(0);
	a.set();
	a.reset(0);
	a.reset();
	return 0;
}
